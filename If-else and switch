#include<stdio.h>
//IF-ELSE STATEMENT
int main(){
	int marks;
	printf("Enter marks:");
	scanf("%d",&marks);
	 if(marks==100){
	 	printf("%d\n",marks);
	 	printf("Very good score");
	 }
	 else if(marks>=80 && marks<=90){
	 	printf("%d\n",marks);
	 	printf("Average score");
	 	
	 }
	 else if(marks>=60 && marks<=80){
	 	printf("%d\n",marks);
	 	printf("good score");
	 	
	 }
	 else{
	 	
	 		printf("very poor");
		 }
		 
	 
	 return 0;

//SWITCH STATEMENT 

	int a,b,c;
	int ch;
	printf("Enter two numbers:");
	scanf("%d%d",&a,&b);
	printf("Enter choice");
	
	scanf("%d",&ch);
	switch(ch){
		case 1:
			c=a+b;
			printf("%d\n",c);
			break;
		case 2:
			c=a*b;
			printf("%d\n",c);
			break;
		case 3:
			c=b/a;
			printf("%d\n",c);
			break;
		case 4:
			c=a-b;
			printf("%d\n",c);
			break;
			
	}
	
	
}
